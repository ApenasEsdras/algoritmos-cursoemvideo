algoritmo "semnome"

var
V1,V2,S: Inteiro

Funcao Soma(X, Y: Inteiro): Inteiro
Inicio
      RETORNE X + Y
FimFuncao
inicio
      Escreva("Digite o primeiro numero: ")
      Leia(V1)
      Escreval("Digite o segundo numero: ")
      Leia(V2)
      S<- Soma(V1,V2)
      Escreval("A soma entre,", V1, " e ", V2, " é igual a ", S)
fimalgoritmo

algoritmo "semnome"
// Função :
// Autor :
// Data : 26/07/2022
// Seção de Declarações
var
    idade, ano, anoAtual: real
inicio

      escreval("digite o ano que vc Nasceu")
      leia(ano)
      escreval("digite o ano que estamos ")
      leia(anoAtual)
      idade<- anoAtual - ano
      escreval("vc tem ", idade, " de idade")
// Seção de Comandos
fimalgoritmo

algoritmo "semnome"
// Função :
// Autor :
// Data : 26/07/2022
// Seção de Declarações
var
contador, conteAte: inteiro
inicio
      ESCREVAL("Quer contar ate qunto? ")
      leia(conteAte)
      contador <- 0
      enquanto (contador <=conteAte) faca
               escreval(contador)
               contador <- contador + 1
      fimenquanto
// Seção de Comandos
fimalgoritmo
